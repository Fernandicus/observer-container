{
  "compilerOptions": {
    "target": "ES2021",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true ,
    "strict": true ,
    "skipLibCheck": true,
    "outDir": "dist"
  },
  "include": ["lib"],
  "exclude": ["node_modules", "dist"]
}
